{
  "mcpServers": {
    "cascade-thinking": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "cascade-thinking-mcp"]
    },
    "jetbrains": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@jetbrains/mcp-proxy"]
    },
    "webresearch": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@mzxrai/mcp-webresearch@latest"]
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@playwright/mcp@latest", "--allowed-hosts=*", "--isolated"]
    }
  }
}
