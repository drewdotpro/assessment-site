{
  "name": "@onwidget/astrowind",
  "version": "1.0.0-beta.52",
  "description": "AstroWind: A free template using Astro 5.0 and Tailwind CSS. Astro starter theme.",
  "type": "module",
  "private": true,
  "engines": {
    "node": "^18.17.1 || ^20.3.0 || >= 21.0.0"
  },
  "scripts": {
    "predev:assessments": "npm run prepare:assessments && kill-port 4321 8081",
    "dev:assessments": "cross-env WEBSITE_ID=assessments npm run _dev:with-cms",
    "predev:adhd": "npm run prepare:adhd && kill-port 4321 8081",
    "dev:adhd": "cross-env WEBSITE_ID=adhd npm run _dev:with-cms",
    "predev:autism": "npm run prepare:autism && kill-port 4321 8081",
    "dev:autism": "cross-env WEBSITE_ID=autism npm run _dev:with-cms",
    "_dev:with-cms": "cross-env ASSESSMENTS_URL=http://localhost:4321 ADHD_URL=http://localhost:4321 AUTISM_URL=http://localhost:4321 concurrently -k --success first --handle-input \"npx decap-server\" \"astro dev --host\"",
    "prepare:assessments": "cross-env WEBSITE_ID=assessments npm run _prepare:cms-config",
    "prepare:adhd": "cross-env WEBSITE_ID=adhd npm run _prepare:cms-config",
    "prepare:autism": "cross-env WEBSITE_ID=autism npm run _prepare:cms-config",
    "_prepare:cms-config": "cross-env ASSESSMENTS_URL=http://localhost:4321 ADHD_URL=http://localhost:4321 AUTISM_URL=http://localhost:4321 node scripts/generate-cms-config.js",
    "prebuild:assessments": "npm run prepare:assessments",
    "build:assessments": "cross-env WEBSITE_ID=assessments astro build",
    "prebuild:adhd": "npm run prepare:adhd",
    "build:adhd": "cross-env WEBSITE_ID=adhd astro build",
    "prebuild:autism": "npm run prepare:autism",
    "build:autism": "cross-env WEBSITE_ID=autism astro build",
    "build:icon-data": "node scripts/build-icon-data.js",
    "preview": "astro preview",
    "astro": "astro",
    "precommit": "npm run fix && npm run check:astro",
    "check": "npm run check:astro && npm run check:eslint && npm run check:prettier",
    "check:astro": "cross-env WEBSITE_ID=assessments ASSESSMENTS_URL=http://localhost:4321 ADHD_URL=http://localhost:4321 AUTISM_URL=http://localhost:4321 astro check",
    "check:eslint": "eslint .",
    "check:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint --fix .",
    "fix:prettier": "prettier -w .",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.12",
    "@astrojs/sitemap": "^3.4.2",
    "@astrolib/analytics": "^0.6.1",
    "@astrolib/seo": "^1.0.0-beta.8",
    "astro": "^5.12.9",
    "astro-embed": "^0.9.0",
    "astro-icon": "^1.1.5",
    "limax": "4.1.0",
    "lodash.merge": "^4.6.2",
    "marked": "^16.4.0",
    "unpic": "^4.1.3"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.3",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/tailwind": "^5.1.5",
    "@eslint/js": "^9.33.0",
    "@iconify-json/flat-color-icons": "^1.2.1",
    "@iconify-json/tabler": "^1.2.20",
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/mdx": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "astro-compress": "2.3.8",
    "astro-eslint-parser": "^1.2.2",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "decap-server": "^3.3.1",
    "eslint": "^9.33.0",
    "eslint-plugin-astro": "^1.3.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "kill-port": "^2.0.1",
    "mdast-util-to-string": "^4.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "reading-time": "^1.5.0",
    "sharp": "0.34.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.39.0",
    "unist-util-visit": "^5.0.0"
  }
}
